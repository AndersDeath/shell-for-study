{
  "name": "shell-for-study",
  "version": "0.9.36",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:production": "ng build --configuration production",
    "gh-prepare": "ng build --configuration production --base-href https://andersdeath.github.io/shell-for-study/",
    "gh-deploy": "ngh --dir=dist/shell-for-study/",
    "gh": "npm run gh-prepare && npm run gh-deploy",
    "compodoc": "compodoc -p tsconfig.doc.json -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.2",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "~13.2.2",
    "@angular/compiler": "~13.2.2",
    "@angular/core": "~13.2.2",
    "@angular/forms": "~13.2.2",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "~13.2.2",
    "@angular/platform-browser-dynamic": "~13.2.2",
    "@angular/router": "~13.2.2",
    "@angular/service-worker": "~13.2.2",
    "@ngrx/store": "^13.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "ngx-markdown": "^13.1.0",
    "ngx-ui-loader": "^11.0.0",
    "rxjs": "~6.6.0",
